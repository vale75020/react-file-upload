{
  "homepage": "https://vale75020.github.io/react-file-upload",
  "name": "react-file-upload",
  "version": "1.0.0",
  "description": "\"a react and express file uploader\"",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Valentino Suma",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "gh-pages": "^2.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2"
  }
}
